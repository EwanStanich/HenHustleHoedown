script/source = "extends Node2D

@onready var anim = $ColorRect/AnimationPlayer
@onready var tutePlayer = $Tutorial/Player
var token_http_request
var score_http_request
var reconnect_http_request
var titleAnim
var inBed = false
var pageTwo = false
var pageFour = false
var inBedArea = false
var tutPage = 1

func _ready():
	if Game.isFirstInstance:
		$Title.z_index = 4
	else:
		$Title.z_index = 0
	$Title.visible = true
	titleAnim = $Title/AnimationPlayer
	titleAnim.play(\"Fade\")
	if !Game.playerToken:
		token_http_request = $TokenHTTPRequest
		get_player_token(false)
	else:
		ready_2()


func ready_2():
	reconnect_http_request = $ReconnectHTTPRequest
	score_http_request = $ScoreHTTPRequest
	#Utils.saveGame()
	Utils.loadGame()
	Input.set_mouse_mode(Input.MOUSE_MODE_VISIBLE)	
	anim.play(\"Open\")
	$Player.play(\"Run\")
	$Chicken.play(\"Run\")
	if Game.isOffline:
		$Buttons/HighScore.visible = false
		$Buttons/Offline.visible = true
		$Buttons/Reconnect.visible = true
	else:
		$Buttons/HighScore.visible = true
		$Buttons/Offline.visible = false
		$Buttons/Reconnect.visible = false
	if Game.isFirstInstance:
		Game.isFirstInstance = false
		titleAnim.play(\"Move\")
		await titleAnim.animation_finished
		$Buttons/AnimationPlayer.play(\"Fade\")
		$Title.z_index = 0
	else:
		revis_title_screen()
		titleAnim.play(\"Move2\")
	close_hs_board()
	if Game.level1:
		$\"Level_Buttons/Level 2\".disabled = false
		$\"HS_Buttons/HS Level 1\".disabled = false
	if Game.level2:
		$\"HS_Buttons/HS Level 2\".disabled = false
		$\"Level_Buttons/Level 3\".disabled = false
	if Game.level3:
		$\"HS_Buttons/HS Level 3\".disabled = false
		$\"Level_Buttons/Level 4\".disabled = false
	if Game.level4:
		$\"HS_Buttons/HS Level 4\".disabled = false
		$\"Level_Buttons/Level 5\".disabled = false		
	if Game.level5:
		$\"HS_Buttons/HS Level 5\".disabled = false
		$\"Level_Buttons/Level 6\".disabled = false
	if Game.level6:
		$\"HS_Buttons/HS Level 6\".disabled = false
		$\"Level_Buttons/Level 7\".disabled = false
	if Game.level7:
		$\"HS_Buttons/HS Level 7\".disabled = false
		$\"Level_Buttons/Level 8\".disabled = false
	if Game.level8:
		$\"HS_Buttons/HS Level 8\".disabled = false
		$\"Level_Buttons/Level 9\".disabled = false
	if Game.level9:
		$\"HS_Buttons/HS Level 9\".disabled = false
	
	$Title.visible = true
	$Buttons.visible = true
	
 

func get_player_token(isReconnect):
	var url = \"https://api.lootlocker.io/game/v2/session/guest\"
	var header = []
	var method = HTTPClient.METHOD_POST
	var request_data = {
	\"game_key\": \"dev_9aa726187e614b59a9c6fcc28131cd8e\",
	\"game_version\": \"1.0.0\",
	\"player_identifier\": \"1\",
	\"development_mode\": true
	}
	if isReconnect:
		reconnect_http_request.request(url, header, method, JSON.stringify(request_data))
	else:
		token_http_request.request(url, header, method, JSON.stringify(request_data))


func _on_http_request_request_completed(result, response_code, headers, body):
	if response_code != 200:
		await get_tree().create_timer(1.5).timeout
		await titleAnim.animation_finished
		Game.isOffline = true
	else:
		Game.isOffline = false
		Game.hasConnected = true
		var json_object = JSON.new()
		body = body.get_string_from_utf8()
		json_object.parse(body)
		Game.playerToken = json_object.get_data()[\"session_token\"]
		print(\"Player token: \", Game.playerToken)		
	ready_2()


func _on_play_button_up():
	$Level_Buttons.visible = true
	$Title.visible = false
	$Buttons.visible = false	


func _on_tutorial_button_up():
	tutPage = 1
	$Tutorial/Right.visible = true
	$Tutorial/Left.visible = false
	tutePlayer.position = Vector2(169,59)
	$\"Tutorial/Page 1/ChickensStay\".visible = false
	$\"Tutorial/Page 1/Chicken\".set_collision_mask_value(2, false)
	$\"Tutorial/Page 1/Chicken2\".set_collision_mask_value(2, false)
	$\"Tutorial/Page 1/Chicken\".position = Vector2(85,95)
	$\"Tutorial/Page 1/Chicken2\".position = Vector2(251,122)
	tutePlayer.set_collision_mask_value(6, false)
	tutePlayer.set_collision_mask_value(1, true)
	tutePlayer.set_collision_layer_value(6, false)
	tutePlayer.set_collision_layer_value(1, true)
	play_keys()
	$\"Tutorial/Page 1\".visible = true
	$\"Tutorial/Page 2\".visible = false
	$\"Tutorial/Page 3\".visible = false
	$\"Tutorial/Page 4\".visible = false
	$Tutorial.visible = true
	$Title.visible = false
	$Buttons.visible = false
	pageTwo = false
	pageFour = false


func _on_quit_button_up():
	get_tree().quit()


func _on_level_1_button_up():
	get_tree().change_scene_to_file(\"res://Levels/game_level.tscn\")


func _on_back_button_up():
	$Level_Buttons.visible = false
	$HS_Buttons.visible = false
	$Tutorial.visible = false
	$Title.visible = true
	$Buttons.visible = true
	tutePlayer.hide_arrow()
	tutePlayer.hide_sleep()


