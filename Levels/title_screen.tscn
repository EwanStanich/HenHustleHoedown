func _on_score_http_request_request_completed(result, response_code, headers, body):
	var json_object = JSON.new()
	body = body.get_string_from_utf8()
	json_object.parse(body)
	var scores = [\"--:--.--\", \"--:--.--\", \"--:--.--\", \"--:--.--\", \"--:--.--\"]
	if json_object.get_data()[\"items\"]:
		var val = 0
		for score in json_object.get_data()[\"items\"]:
			scores[val] = score[\"metadata\"]
			val += 1
	print(scores)
	$HighScores/HS1.text = scores[0]
	$HighScores/HS2.text = scores[1]
	$HighScores/HS3.text = scores[2]
	$HighScores/HS4.text = scores[3]
	$HighScores/HS5.text = scores[4]


func get_scores(key):
	$HighScores.visible = true
	$HS_Buttons.visible = false
	var url = \"https://api.lootlocker.io/game/leaderboards/%s/list?count=5\" % key
	var header = [\"Content-Type: application/json\", \"x-session-token: %s\" % Game.playerToken]
	var method = HTTPClient.METHOD_GET
	score_http_request.request(url, header, method)	


func close_hs_board():
	$HighScores/HS1.text = \"\"
	$HighScores/HS2.text = \"Loading...\"
	$HighScores/HS3.text = \"\"
	$HighScores/HS4.text = \"\"
	$HighScores/HS5.text = \"\"
"

