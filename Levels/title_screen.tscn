func _on_score_http_request_request_completed(result, response_code, headers, body):
	if response_code != 200:
		await get_tree().create_timer(1.5).timeout
		Game.isOffline = true
		$Buttons/HighScore.visible = false
		$Buttons/Offline.visible = true
		$Buttons/Reconnect.visible = true
		$HighScores.visible = false
		$Title.visible = true
		$Buttons.visible = true
	else:
		var json_object = JSON.new()
		body = body.get_string_from_utf8()
		json_object.parse(body)
		var scores = []
		if json_object.get_data()[\"items\"]:
			var val = 0
			for score in json_object.get_data()[\"items\"]:
				scores.append(score[\"metadata\"])
				val += 1
		print(scores)
		var text_boxes = $HighScores.get_children().slice(1, 17)
		var i = 0
		for box in text_boxes:
			if i < scores.size():
				box.text = scores[i]
			else:
				box.text = \"--:--.--\"
			i += 1
		$HighScores/Loading.visible = false


func get_scores(key):
	$HighScores.visible = true
	$HS_Buttons.visible = false
	var url = \"https://api.lootlocker.io/game/leaderboards/%s/list?count=16\" % key
	var header = [\"Content-Type: application/json\", \"x-session-token: %s\" % Game.playerToken]
	var method = HTTPClient.METHOD_GET
	score_http_request.request(url, header, method)


func close_hs_board():
	$HighScores/Loading.visible = true
	for i in $HighScores.get_children().slice(1, 17):
		i.text = \"\"


func _on_message_back_button_up():
	$Buttons/ErrorMessage.visible = false
	$Buttons/ErrorMessage/TryAgainMessage.visible = false
	$Buttons/ErrorMessage/NormalMessage.visible = true


func _on_reconnect_button_up():
	$Buttons/ErrorMessage.visible = true


func _on_reconnect_token_button_up():
	$Buttons/ErrorMessage/NormalMessage.visible = false
	$Buttons/ErrorMessage/TryAgainMessage.visible = false
	$Buttons/ErrorMessage/LoadingMessage.visible = true
	$Buttons/ErrorMessage/MessageBack.visible = false
	$Buttons/ErrorMessage/ReconnectToken.visible = false
	get_player_token(true)


func _on_reconnect_http_request_request_completed(result, response_code, headers, body):
	if response_code != 200:
		await get_tree().create_timer(1.5).timeout
		$Buttons/ErrorMessage/TryAgainMessage.visible = true
		$Buttons/ErrorMessage/LoadingMessage.visible = false
		$Buttons/ErrorMessage/MessageBack.visible = true
		$Buttons/ErrorMessage/ReconnectToken.visible = true
	elif Game.hasConnected:
		$Buttons/ErrorMessage/LoadingMessage.visible = false
		$Buttons/ErrorMessage/MessageBack.visible = true
		$Buttons/ErrorMessage/ReconnectToken.visible = true
		Game.isOffline = false
		$Buttons/ErrorMessage/NormalMessage.visible = true
		print(\"Player token: \", Game.playerToken)
		$Buttons/ErrorMessage.visible = false
		ready_2()
	else:
		$Buttons/ErrorMessage/LoadingMessage.visible = false
		$Buttons/ErrorMessage/MessageBack.visible = true
		$Buttons/ErrorMessage/ReconnectToken.visible = true
		$Buttons/ErrorMessage/NormalMessage.visible = true
		Game.isOffline = false
		Game.hasConnected = true
		var json_object = JSON.new()
		body = body.get_string_from_utf8()
		json_object.parse(body)
		Game.playerToken = json_object.get_data()[\"session_token\"]
		print(\"Player token: \", Game.playerToken)
		$Buttons/ErrorMessage.visible = false
		ready_2()


