func _input(event):
	if event is InputEventKey:
		if Input.is_key_pressed(KEY_E) and inBed:
			anim.play(\"FadeIn\")
			await anim.animation_finished
			tutePlayer.position = Vector2(169,59)
			tutePlayer.set_down()
			anim.play(\"FadeOut\")
			await anim.animation_finished
		if (Input.is_key_pressed(KEY_TAB) or Input.is_key_pressed(KEY_ESCAPE)) and pageFour:
			$\"Tutorial/Page 4/Pause\".visible = true
			tutePlayer.pause()
			$\"Tutorial/Page 4/TabKey\".pause()
