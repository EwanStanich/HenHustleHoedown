func _on_high_score_button_up():
	$Buttons.visible = false
	$HS_Buttons.visible = true


func _on_hs_back_button_up():
	$HighScores.visible = false
	$HS_Buttons.visible = true
	close_hs_board()


func _on_hs_level_1_button_up():
	$HighScores/LevelNum.text = \"Level 1\"
	get_scores(21217)


func _on_hs_level_2_button_up():
	$HighScores/LevelNum.text = \"Level 2\"
	get_scores(21219)


